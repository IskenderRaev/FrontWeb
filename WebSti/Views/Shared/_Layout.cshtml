    /**____________________________Заглавия_______________**/
    /**____________________________Функция при наведении реагирует и двигает DIVку по вертикали_______________**/
    .baseBlock {
        overflow: hidden;
        background: #fff;
        -moz-transition: all 0.3s cubic-bezier(0.165, 0.84, 0.44, 1);
        -o-transition: all 0.3s cubic-bezier(0.165, 0.84, 0.44, 1);
        transition: all 0.3s cubic-bezier(0.165, 0.84, 0.44, 1);
        box-shadow: 0 4px 20px 1px rgb(0 0 0 / 6%), 0 1px 4px rgb(0 0 0 / 8%);
        border: 0;
        position: relative;
        display: flex;
        flex-direction: column;
    }

    .baseBlock:hover {
        -webkit-transform: translate(0, -8px);
        -moz-transform: translate(0, -8px);
        -ms-transform: translate(0, -8px);
        -o-transform: translate(0, -8px);
        transform: translate(0, -8px);
        box-shadow: 0 40px 40px rgba(0, 0, 0, 0.2);
    }
    /**__________Функция при наведении реагирует и двигает DIVку по вертикали_______________**/
    /**__________Актуальные новости________**/
