    <link rel="stylesheet" href="~/lib/animated-event-calendar/dist/simple-calendar.css">
    <link href="~/lib/select2/css/select2.min.css" rel="stylesheet" />
    <link href="~/lib/select2-bootstrap-theme/select2-bootstrap.css" rel="stylesheet" />
    <link href="~/lib/sweetalert/dist/sweetalert.css" rel="stylesheet" />
    
</head>
<body>
    @using Microsoft.AspNetCore.Builder
    @using Microsoft.AspNetCore.Localization
    @using Microsoft.Extensions.Options

    @inject IOptions<RequestLocalizationOptions> LocOptions    
    @{
        string returnUrl = ViewContext.HttpContext.Request.Path;

        var requestCulture = Context.Features.Get<IRequestCultureFeature>();
        var cultureItems = LocOptions.Value.SupportedUICultures
        .Select(c => new SelectListItem { Value = c.Name, Text = c.NativeName })
        .ToList();
        int yearOfWebsite = DateTime.Now.Year;
    }
    <div style="background: url('../../images/Resourse.png') no-repeat; background-size: cover; background-position: center;">
        <div class="container navbarHeaderTop" style="height:36px">
            <nav class="navbar navbar-expand-lg" style="color:#018A90; ">
                <div class="container p-0">
                    <div class="dropdown text-white">
                        <button class="btn dropdown-toggle" style="background: transparent; color: white;" role="button" id="dropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            @Localize("stsservices")
                        </button>
                        <ul class="dropdown-menu" aria-labelledby="dropdownMenuLink">
                            <li><a class="dropdown-item" href="#">@Localize("allwebsitessts")</a></li>
                            <li><a class="dropdown-item" href="https://www.sti.gov.kg/">@Localize("officialwebsitests")</a></li>
                            <li><a class="dropdown-item" href="https://stat.salyk.kg/">@Localize("opendata")</a></li>
                            <li><a class="dropdown-item" href="https://cabinet.salyk.kg/account/login?ReturnUrl=%2F">@Localize("tpaccount")</a></li>
                            <li><a class="dropdown-item" href="https://import.salyk.kg/">@Localize("consolidatedpost")</a></li>
                            <li><a class="dropdown-item" href="https://esf.salyk.kg/esf/view/user/login.xhtml?faces-redirect=true">@Localize("electronicinvoice")</a></li>
                            <li><a class="dropdown-item" href="https://vat.salyk.kg/">@Localize("vat")</a></li>
                        </ul>
                    </div>
                    <button class="navbar-toggler navbar-dark " type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                    <div class="collapse navbar-collapse navbar-dark" style=" color:#018A90" id="navbarSupportedContent">
                        <div class="navlinksDiv">
                        <ul class="navbar-nav navbarnavUl">
                            <li class="nav-item">
                                <a href="https://www.instagram.com/salyk_kg/" class="nav-link"><i class="bi bi-instagram fs-4 text-white"></i></a>
                            </li>
                            <li class="nav-item">
                                <a href="https://t.me/gnskg" class="nav-link"><i class="bi bi-telegram fs-4 text-white"></i></a>
                            </li>
                            <li class="nav-item">
                                <a href="https://www.facebook.com/salykKG/" class="nav-link"><i class="bi bi-facebook fs-4 text-white"></i></a>
                            </li>
                            <li class="nav-item">
                                <a href="https://www.youtube.com/channel/UCcgL1pBJA6WRGg4yLQ6lbpg" class="nav-link"><i class="bi bi-youtube fs-4 text-white"></i></a>
                            </li>
                            <li class="nav-item">
                                <a href="#" class="nav-link"><i class="bi bi-twitter fs-4 text-white"></i></a>
                            </li>
                            <li class="nav-item">
                                    <a href="#" class="nav-link"> <i class="fab fa-tiktok fs-4 text-white"></i></a>
                            </li>
                            <li class="nav-item ">
                                <span class="ml-1 border-0">
                                    <form asp-action="ChangeLanguage" asp-controller="Home" method="post">
                                    <input type="hidden" id="returnUrl" name="returnUrl" value="@returnUrl" />
                                        <select class="text-right p-0"  style="border:none;background: transparent; margin-bottom: 10px;" id="culture"
                                            name="culture"
                                            class="form-control"
                                            onchange="this.form.submit();"
                                            asp-for="@requestCulture.RequestCulture.UICulture.Name">
                                            <option value="@cultureItems[1].Value">Кыргызча</option>
                                            <option value="@cultureItems[0].Value">Русский</option>
                                            <option value="@cultureItems[2].Value">English</option>
                                        </select>
                                    </form>
                                </span>
                            </li>
                        </ul>
                    </div>
                </div>
                </div>
            </nav>
        </div>
    </div>
    <div class="container navbarTitleMain mt-3" style="min-height: 100vh;">
        <nav class="navbar navbar-expand-md">
            <div class="row align-items-center">
                <div class="col-1 d-none d-sm-none d-md-none d-lg-none d-xl-block">
                    <a asp-action="Index" asp-controller="Home"><img class="img-fluid mainHerbLogoFront" src="~/images/mainlogo.png" /> </a>
                </div>
                <div class="col-11 pt-3">
                    <div class="border-bottom">
                        <div class="row align-items-center justify-content-between">
                            <div class="col-sm-12 col-md-12 col-lg-12 col-xl-8 "><a class="navbar-brand" asp-action="Index" asp-controller="Home"><h5 style="color:#018A90; white-space: pre-wrap; ">@Localize("logoname")</h5></a></div>
                            <div class="col-6 col-sm-6 col-md-6 col-lg-6 col-xl-2 text-right p-0"><a class="navbar-brand"><h6><b><i class="bi bi-telephone" style="color:#018A90; margin-right: 5px;"></i><span style="font-size:15px;">@Localize("callcenter"): 116 </span></b></h6></a></div>
                            <div class="col-6 col-sm-6 col-md-6 col-lg-6 col-xl-2 text-right p-0"><a class="navbar-brand"><h6><b><img src="~/images/login.png" style="margin-right: 5px;" /><span style="font-size:15px;">@Localize("myaccount")</span></b></h6></a></div>
                        </div>
                    </div>
                    <div class="row justify-content-between">
                        <div class="col-xl-10 ">
                            <div class="justify-content-start  mt-2">
                                @await Component.InvokeAsync("Navs")
                            </div>
                        </div>
                        <div class="col-xl-2 SearchText pe-0">
                            <form asp-controller="Home" asp-action="SearchText" method="get" autocomplete="off">
                            <div class="input-group mt-1">
                                <input type="text" class="form-control input-group-text" placeholder="Поиск" name="SearchWord"/>
                                <div class="input-group-append">
                                    <span class=""><button class="btn btn-default" style="border-color: #e0d8d8; " type="submit"><i style="color:#018A90;" class="bi bi-search"></i> </button></span>
                                </div>
                            </div>
                            </form>
                        </div>                                        
                    </div>
                </div>
            </div>
        </nav>
        <div class="container border-top mt-2">
            @RenderSection("Styles", required: false)
            @RenderBody()
        </div>
    </div>

    <footer class="Footer" id="footer">
        <div class="pt-4 pb-2" style="border-top: 1px solid #EEEEEE; background-color: #f8f8f8;">
            <div class="container">
                <div class="row pt-2">
                    <div class="col-sm-4">
                        <img src="~/images/Group.svg" class="img-fluid" style="width:60px; height:60px;" />
                        <p class="font-weight-bold" style="font-size:14px;">@Localize("footerlogoname1") <br /> @Localize("footerlogoname2")</p>
                    </div>
                    <div class="col-sm-4">
                        <p><i class="bi bi-geo-alt pr-1" style="color: #018A90;"></i>@Localize("ouraddress")</p>
                        <p><i class="bi bi-telephone pr-1" style="color: #018A90;"></i>@Localize("callcenter"): 116</p>
                        <p><i class="bi bi-telephone-plus pr-1" style="color: #018A90;"></i>@Localize("hotline"): +996 (312) 12-34-56</p>
                        <p><i class="bi bi-envelope pr-1" style="color: #018A90;"></i>E-mail: sti@sti.gov.kg</p>
                    </div>
                    <div class="col-sm-4">
                        <p class="">@Localize("technicalsupport")</p>
                        <p class=""><i class="bi bi-telephone pr-1" style="color: #018A90;"></i>@Localize("callcenter"): +996 508 996 669, <br /> +996 755 000 116, +996 997 000 116</p>
                        <p class="">@Localize("sitedevelopedby"):  <img class="pl-2" src="~/images/salykIcon.png" /></p>
                    </div>
                </div>
            </div>
        </div>
        <div style="background: url('../../images/Resourse.png') no-repeat; background-size: cover; background-position: center;" class="pb-0">
            <div class="container text-center pt-2 pb-2">
                <p class="pb-0 text-white">&copy; @yearOfWebsite @Localize("websiteStateTaxService"). @Localize("allrightsreserved")</p>
            </div>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/lib/font-awesome/js/fontawesome.js"></script>
    <script src="~/lib/font-awesome/js/brands.js"></script>
    <script type="text/javascript" src="~/js/lc_lightbox.lite.js"></script>
    <script src="~/js/lightbox.min.js"></script>
    <script src="~/lib/swiper/swiper-bundle.min.js"></script>
    @await Component.InvokeAsync("Notyf")
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
