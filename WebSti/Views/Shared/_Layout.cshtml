        padding: 10px 15px;
    }

    .NewsActual {
        border-left: 3px solid #018A90;
        border-radius: 0;
    }

    .contentt p {
        color: #018A90;
    }

    .contentt a {
        color: black;
    }

    .cont {
        padding-left: 0 0 0px 10px;
        margin-bottom: 15px;
    }
    /**__________Актуальные новости________**/
    /**__________Стиль на картинку слайдер новости________**/
    .CarouselImage {
        z-index: 1;
    }

    .carousel-caption {
        position: absolute;
        right: 0;
        left: 0;
        bottom: 0;
        z-index: 10;
        padding-top: 20px;
        padding-bottom: 20px;
        color: #fff;
        text-align: center;
        opacity: 1;
    }

    .card .CarouselTitle a {
        color: white;
        opacity: 100%;
    }

    .card .CarouselTitle {
        background-color: black;
        opacity: 70%;
    }

    .carousel-indicators {
        position: absolute;
        top: 0;
        left: auto;
        margin: 15px 15px 0 0;
        bottom: 10px;
        display: flex;
        padding-left: 0;
        list-style: none;
        z-index: 0;
    }

    .carousel img {
        width: 857px; /* you can use % */
        height: 504px;
    }
    /**__________Стиль на картинку слайдер новости________**/
    /**__________Календарь________**/
    .calendar {
        height: 403px;
    }
    .calendar td {
         padding: .1em .1em; 
    }
    .calendar thead td {
            color: #018A90;
        }
    .calendar header .month {
        padding: 20px;
        margin: 0;
    }
    .calendarItem {
        max-width: auto;
    }

    .calendar .day.has-event:after {
        content: '';
        position: absolute;
        top: calc(20%);
        left: calc(80% - 2px);
        width: 8px;
        height: 8px;
        border-radius: 50%;
        background: #d71d50;
    }

    .calendar .day.today.has-event:after {
        background: #d71d50;
    }

    .calendar .event-container {
        display: none;
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        padding: 60px 30px;
        background: #545A5C;
        box-sizing: border-box;
    }

    .calendar header .btn-prev {
        top: 20px;
        left: 90px;
        transform: rotate(-180deg);
    }
    .calendar header .btn-next {
        top: 20px;
        right: 90px;
    }

    .calendar header .simple-calendar-btn:hover {
        background: none;
        color: black;
    }
    .calendar header .simple-calendar-btn {
        display: inline-block;
        position: absolute;
        width: 30px;
        height: 30px;
        text-align: center;
        line-height: 30px;
        color: black;
        border-radius: 50%;
        border: none;
    }
    .calendar header .simple-calendar-btn:before {
        content: '';
        position: absolute;
        top: 9px;
        left: 8px;
        width: 12px;
        height: 12px;
        border-style: solid;
        border-width: 3px 3px 0 0;
        transform: rotate(45deg);
        transform-origin: center center;
    }

    .calendar .day.today {
        background: transparent;
        color: #222020;
        font-weight: bold;
        position: relative;
        display: inline-block;
        width: 2.5em;
        height: 2.5em;
        line-height: 2.5em;
        border-radius: 10%;
        border: 1px solid #018A90;
        cursor: pointer;
    }
